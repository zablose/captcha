{
    "name": "zablose/captcha",
    "description": "Simple captcha with optional goodies for Laravel.",
    "keywords": [
        "captcha",
        "security",
        "php",
        "laravel",
        "spam",
        "bot"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Sergejs Zablockis",
            "email": "zablose@gmail.com"
        }
    ],
    "require": {
        "php": "~7.4",
        "ext-gd": "*"
    },
    "require-dev": {
        "facade/ignition": "^2.0",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "fzaninotto/faker": "^1.9.1",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/framework": "^7.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.0",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^9.0.2"
    },
    "autoload": {
        "psr-4": {
            "Zablose\\Captcha\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zablose\\Captcha\\Laravel\\App\\": "laravel/app/",
            "Zablose\\Captcha\\Laravel\\Tests\\": "laravel/tests/"
        },
        "classmap": [
            "laravel/database/factories",
            "laravel/database/seeds"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zablose\\Captcha\\CaptchaServiceProvider"
            ]
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
