{
    "name": "zablose/captcha",
    "description": "Simple captcha with optional goodies for Laravel.",
    "keywords": [
        "captcha",
        "security",
        "php",
        "laravel",
        "spam",
        "bot"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Sergejs Zablockis",
            "email": "zablose@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-gd": "*"
    },
    "require-dev": {
        "ext-pdo": "*",
        "facade/ignition": "^2.12.0",
        "fideloper/proxy": "^4.4.1",
        "fakerphp/faker": "^1.15.0",
        "guzzlehttp/guzzle": "^7.3.0",
        "laravel/framework": "^8.56.0",
        "laravel/tinker": "^2.6.1",
        "laravel/ui": "^3.3.0",
        "mockery/mockery": "^1.4.3",
        "nunomaduro/collision": "^5.8.0",
        "phpunit/phpunit": "^9.5.8"
    },
    "autoload": {
        "psr-4": {
            "Zablose\\Captcha\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "laravel/app/",
            "Database\\Factories\\": "laravel/database/factories/",
            "Tests\\": "laravel/tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zablose\\Captcha\\CaptchaServiceProvider"
            ]
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
